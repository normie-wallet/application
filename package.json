{
  "name": "normie-wallet-mobile",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prebuild": "expo prebuild",
    "build:ios": "expo run:ios",
    "build:android": "expo run:android"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-navigation/native": "^7.1.10",
    "@react-navigation/native-stack": "^7.3.14",
    "crypto-browserify": "^3.12.1",
    "expo": "~53.0.0",
    "expo-auth-session": "~6.2.0",
    "expo-barcode-scanner": "^13.0.1",
    "expo-camera": "^16.1.8",
    "expo-dev-client": "^5.2.0",
    "expo-font": "^13.3.1",
    "expo-linear-gradient": "^14.1.5",
    "expo-modules-core": "^2.4.0",
    "expo-random": "^14.0.1",
    "expo-secure-store": "~14.2.3",
    "expo-sensors": "^14.1.4",
    "expo-status-bar": "~2.2.3",
    "expo-web-browser": "~14.1.6",
    "node-libs-react-native": "^1.2.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.3",
    "react-native-camera": "^4.2.1",
    "react-native-get-random-values": "^1.11.0",
    "react-native-qrcode": "^0.2.7",
    "react-native-qrcode-scanner": "^1.5.5",
    "react-native-qrcode-svg": "^6.3.15",
    "react-native-reanimated": "^3.18.0",
    "react-native-safe-area-context": "^5.4.1",
    "react-native-screens": "^4.11.1",
    "react-native-sensors": "^7.3.6",
    "react-native-svg": "^15.12.0",
    "react-native-url-polyfill": "^1.3.0",
    "react-native-vector-icons": "^10.2.0",
    "stream-browserify": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~19.0.10",
    "@types/react-native-vector-icons": "^6.4.18",
    "react-native-dotenv": "^3.4.11",
    "typescript": "^5.1.3"
  },
  "private": true,
  "expo": {
    "scheme": "normiewallet",
    "ios": {
      "bundleIdentifier": "com.normie-wallet.normie-wallet-mobile",
      "infoPlist": {
        "NFCReaderUsageDescription": "We need to use NFC for contactless payments",
        "com.apple.developer.nfc.readersession.formats": [
          "NDEF",
          "TAG"
        ]
      }
    },
    "android": {
      "package": "com.normie-wallet.normi-wallet-mobile",
      "permissions": [
        "NFC"
      ]
    }
  }
}
